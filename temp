(defun compact-function-toggle ()
  (setq compact (not compact))
  (search-forward "Function:")
  (let*(
        (start (point))
        )
    (move-end-of-line 1)
    (kill-ring-save start (point))
    )
  (re-print)
  (search-forward (car kill-ring))
  (search-backward "----")
  (move-beginning-of-line 1)
  (recenter (nth 1 recenter-positions))
